PUSHL.OD   reg32 
            *32* %esp := %esp - 4
            *32* m[%esp] := reg32;

PUSHL.CS
            *32* %esp := %esp - 4
            *16* m[%esp] := %cs;
	    
PUSHL.DS
            *32* %esp := %esp - 4
            *16* m[%esp] := %ds;

PUSHL.ES
