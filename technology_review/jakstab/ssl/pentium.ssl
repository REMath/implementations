# Called POPAD in Intel manual
POPA
            *32* %edi := m[%esp]{32}
            *32* %esi := m[%esp + 4]{32}
            *32* %ebp := m[%esp + 8]{32}
            # %esp slot is skipped
            *32* %ebx := m[%esp + 16]{32}
            *32* %edx := m[%esp + 20]{32}
            *32* %ecx := m[%esp + 24]{32}
            *32* %eax := m[%esp + 28]{32}
            *32* %esp := %esp + 32;

#PUSHA
